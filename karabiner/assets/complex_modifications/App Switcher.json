{
  "title": "Leader Key",
  "rules": [
    {
      "description": "L-Ctrl (Non-Apple) / fn (Apple)",
      "manipulators": [
        {
          "type": "basic",
          "description": "Non-Apple Keyboard",
          "from": {
            "key_code": "left_control"
          },
          "to": [
            {
              "key_code": "left_control"
            }
          ],
          "parameters": {
            "basic.to_if_alone_timeout_milliseconds": 600
          },
          "conditions": [
            {
              "type": "device_unless",
              "identifiers": [
                {
                  "is_built_in_keyboard": true,
                  "is_keyboard": true
                }
              ]
            }
          ],
          "to_if_alone": [
            {
              "key_code": "f12",
              "modifiers": "fn"
            }
          ]
        },
        {
          "type": "basic",
          "description": "Apple Keyboard",
          "from": {
            "key_code": "fn"
          },
          "to": [
            {
              "key_code": "fn"
            }
          ],
          "conditions": [
            {
              "type": "device_if",
              "identifiers": [
                {
                  "is_built_in_keyboard": true,
                  "is_keyboard": true
                }
              ]
            }
          ],
          "parameters": {
            "basic.to_if_alone_timeout_milliseconds": 600
          },
          "to_if_alone": [
            {
              "key_code": "f12",
              "modifiers": "fn"
            }
          ]
        }
      ]
    }
  ]
}
