{
  "mcpServers": {
    "fetch": {
      "args": [
        "mcp-server-fetch"
      ],
      "command": "uvx",
      "disabled": false
    },
    "figma": {
      "args": [
        "-y",
        "figma-developer-mcp",
        "--figma-api-key=${FIGMA_API_KEY}",
        "--stdio"
      ],
      "command": "npx",
      "disabled": false,
      "env": {
        "FIGMA_API_KEY": "${FIGMA_API_KEY}"
      }
    },
    "sequentialthinking": {
      "args": [
        "run",
        "--rm",
        "-i",
        "mcp/sequentialthinking"
      ],
      "command": "docker",
      "disabled": false
    }
  },
  "nativeMCPServers": {
    "mcphub": {
      "disabled": true,
      "disabled_tools": [
        "toggle_mcp_server"
      ]
    },
    "neovim": {
      "disabled": false,
      "disabled_prompts": [
        "parrot"
      ],
      "disabled_resources": [],
      "disabled_tools": [
        "list_directory",
        "delete_items",
        "edit_file",
        "execute_command",
        "execute_lua",
        "move_item",
        "write_file"
      ]
    }
  }
}
