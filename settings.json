{
    "security.workspace.trust.untrustedFiles": "open",
    "files.autoSave": "onFocusChange",
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.codeLens.enabled": false,
    "workbench.preferredDarkColorTheme": "GitHub Dark",
    "workbench.preferredHighContrastColorTheme": "GitHub Dark High Contrast",
    "workbench.preferredHighContrastLightColorTheme": "GitHub Dark High Contrast",
    "workbench.preferredLightColorTheme": "GitHub Light Default",
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "git.autofetch": true,
    "python.defaultInterpreterPath": "~/.pyenv/shims/python",
    "editor.fontFamily": "'Monaspace Neon', monospace",
    "terminal.integrated.fontFamily": "'Monaspace Neon', monospace",
    "editor.minimap.enabled": false,
    "editor.fontLigatures": "'calt', 'liga', 'dlig', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08'",
    "editor.renderControlCharacters": true,
    "editor.showFoldingControls": "always",
    "editor.snippetSuggestions": "top",
    "emmet.includeLanguages": {
        "erb": "html"
    },
    "emmet.showSuggestionsAsSnippets": true,
    "emmet.triggerExpansionOnTab": true,
    "explorer.confirmDelete": false,
    "files.exclude": {
        "__pycache__": true,
        "_site": true,
        ".asset-cache": true,
        ".bundle": true,
        ".ipynb_checkpoints": true,
        ".pytest_cache": true,
        ".sass-cache": true,
        ".svn": true,
        "**/.DS_Store": true,
        "**/.egg-info": true,
        "**/.git": true,
        "build": true,
        "coverage": true,
        "dist": true,
        "log": true,
        "node_modules": true,
        "public/packs": true,
        "tmp": true
    },
    "files.hotExit": "off",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.watcherExclude": {
        "**/audits/**": true,
        "**/coverage/**": true,
        "**/log/**": true,
        "**/node_modules/**": true,
        "**/tmp/**": true,
        "**/vendor/**": true
    },
    "git.openRepositoryInParentFolders": "never",
    "autoDarkMode.lightTheme": "GitHub Light Default",
    "autoDarkMode.darkTheme": "GitHub Dark Default",
    "window.autoDetectHighContrast": false,
    "codestream.serverUrl": "https://codestream-us1.service.newrelic.com",
    "workbench.startupEditor": "none",
    "editor.parameterHints.enabled": false,
    "jupyter.themeMatplotlibPlots": true,
    "python.analysis.autoImportCompletions": true,
    "python.pylanceLspNotebooksEnabled": true,
    "editor.quickSuggestions": {
        "strings": "inline"
    },
    "[python]": {
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.bracketPairColorization.enabled": true,
        "editor.tabSize": 4
    },
    "window.autoDetectColorScheme": true,
    "pylint.showNotifications": "always",
    "black-formatter.showNotifications": "always",
    "flake8.showNotifications": "always",
    "autopep8.showNotifications": "always",
    "jupyter.askForKernelRestart": false,
    "notebook.diff.ignoreMetadata": true,
    "notebook.lineNumbers": "on",
    "explorer.confirmDragAndDrop": false,
   // "prettier.useTabs": true,
    "ruff.showNotifications": "onWarning",
    "editor.defaultFormatter": "charliermarsh.ruff",
    "RainbowBrackets.depreciation-notice": false,
    "workbench.colorCustomizations": {

    },
    "workbench.colorTheme": "GitHub Light Default",
    "editor.inlineSuggest.showToolbar": "always",
    "jupyter.interactiveWindow.textEditor.executeSelection": true,
    "woke.enable": false,
    "todohighlight.isEnable": true,
    "todohighlight.defaultStyle": {

    },
    "todohighlight.isCaseSensitive": false,
    "todohighlight.include": [
        "**/*.py",
        "**/*.md",
        "**/*.js",
        "**/*.jsx",
        "**/*.ts",
        "**/*.tsx",
        "**/*.html",
        "**/*.php",
        "**/*.css",
        "**/*.scss",
        "**/*.ipynb"
    ],
    "python.languageServer": "Pylance",
    "python.autoComplete.extraPaths": [
        "/usr/local/bin/python3",
